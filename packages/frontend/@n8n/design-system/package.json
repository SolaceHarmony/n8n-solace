    "format:check": "biome ci .  && prettier --check . --ignore-path ../../../../.prettierignore",
    "lint": "eslint src --ext .js,.ts,.vue --quiet",
    "lintfix": "eslint src --ext .js,.ts,.vue --fix"
  },
  "devDependencies": {
