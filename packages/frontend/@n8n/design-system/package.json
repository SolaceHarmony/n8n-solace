  "main": "src/index.ts",
  "import": "src/index.ts",
  "scripts": {
    "dev": "npm run storybook",
    "clean": "rimraf dist .turbo",
    "build": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest run",
    "test:dev": "vitest",
    "build:storybook": "storybook build",
