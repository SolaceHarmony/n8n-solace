{
	"extends": ["./tsconfig.json", "@n8n/typescript-config/tsconfig.build.json"],
	"compilerOptions": {
		"composite": true,
		"rootDir": "src",
		"outDir": "dist",
		"tsBuildInfoFile": "dist/build.tsbuildinfo",
		"lib": ["es2022"],
		"skipLibCheck": true
	},
	"include": ["src/**/*.ts"],
	"exclude": ["src/**/__tests__/**"]
}
